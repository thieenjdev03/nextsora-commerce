{
  "name": "@nextsora/utils",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/ --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "tailwind-merge": "^2.2.2"
  },
  "devDependencies": {
    "@nextsora/config": "workspace:*",
    "@types/node": "^20.11.24",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.0"
  }
}